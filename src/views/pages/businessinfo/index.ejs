        <button class="btn btn-primary input-group-btn" id="btn-loading" onclick="businessinfoheader()">Search</button>
        <button class="btn btn-primary input-group-btn disabled" onclick="printArea()" id="btnprint">Print</button>
      </div>
      <div class="input-group col-6">
        <label class="form-switch">From Start
          <input type="checkbox" onclick="enablePreviousMonth()"><i class="form-icon"></i>
        </label>
      </div>
    </div>
    <div class="container">
      <br>
      <div class="columns" id="output">
      </div>
    </div>
