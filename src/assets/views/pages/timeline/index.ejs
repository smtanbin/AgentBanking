<main class="content m-2">
    <ul class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="/">Home</a>
        </li>
        <li class="breadcrumb-item">
            <a href="/timeline">Timeline</a>
        </li>

    </ul>

    <div data-aos="zoom-out" class="hero-sm">
        <div class="hero-body">
            <h1 class="text-primary text-bold">Timeline</h1>
        </div>

        <div class="column col-6 col-xs-12">
            <div class="input-group">
                <input class="form-input w100 input-lg col-8" type="text" id="search" placeholder="Search.....">
                <span class="input-group-addon addon-lg bg-gray">Optation</span>
                <select class="form-select select-lg col-2 " id="map_output"></select>
                <button class="btn btn-primary input-group-btn btn-lg" onclick="searchtr()">Submit</button>
            </div>
        </div>
    </div>

    <section style="margin-top: 50px;">
        <div class="modal modal-xl" id="modal-id">
            <a class="modal-overlay" aria-label="Close"></a>
            <div class="modal-container" id="output">
            </div>
        </div>
    </section>

    <section data-aos="fade-down" class="container p-2">
        <div class="columns col-12">
            <div class="column col-lg-6 col-md-12">
                <div class="card p-1">
                    <table class="table table-striped table-cluster">
                        <thead>
                            <tr>
                                <th class="text-tiny">Time</th>
                                <th class="text-tiny">TR No</th>

                                <th class="text-tiny">From AC</th>
                                <th class="text-tiny text-clip mx-1">From GL</th>
                                <th class="text-tiny">To AC</th>
                                <th class="text-tiny text-clip mx-1">To GL</th>
                                <th class="text-tiny">Amount</th>

                                <th class="text-tiny">Merchant</th>
                                <th class="text-tiny">Ref</th>

                                <th class="text-tiny">Particular</th>
                            </tr>
                        </thead>
                        <tbody id="timeline" class="container">

                        </tbody>

                        </tbody>
                    </table>
                    <span class="loading loading-lg" id="timeline-loading"></span>

                </div>
            </div>
        </div>
    </section>
    <script src="/JS/timeline/index.js"></script>
</main>